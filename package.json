{
  "name": "dailyiu",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "make:icon": "npx @bam.tech/react-native-make icon --path ./src/assets/logo/daily-iu-app-icon.png",
    "android:build:debug": "cd android && ./gradlew assembleDebug",
    "android:build:release": "cd android && ./gradlew assembleRelease",
    "android:build:release:apk": "cd android && ./gradlew assembleRelease",
    "android:build:release:bundle": "cd android && ./gradlew bundleRelease",
    "android:build:release:signed": "cd android && ./gradlew assembleRelease",
    "android:build:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --sourcemap-output android/app/src/main/assets/index.android.bundle.map",
    "android:build:standalone": "npm run android:build:bundle && cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:keystore:generate": "cd android/app && keytool -genkeypair -v -storetype PKCS12 -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000 -storepass android -keypass android && echo 'Keystore generated! IMPORTANT: Update gradle.properties with your actual passwords and store the keystore securely.'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "^20.5.0",
    "@react-native-firebase/messaging": "^20.5.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.14",
    "@tanstack/react-query": "^5.80.6",
    "axios": "^1.9.0",
    "moment": "^2.30.1",
    "phosphor-react-native": "^2.3.1",
    "react": "19.0.0",
    "react-hook-form": "^7.57.0",
    "react-native": "0.79.3",
    "react-native-date-picker": "5.0.12",
    "react-native-dotenv": "^3.4.11",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.3.1",
    "react-native-print": "^0.11.0",
    "react-native-reanimated": "^4.0.2",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-video": "^6.16.1",
    "react-native-video-controls": "^2.8.1",
    "react-native-worklets": "^0.4.1",
    "styled-components": "^6.1.18",
    "uuid": "^13.0.0",
    "zod": "^3.25.67"
